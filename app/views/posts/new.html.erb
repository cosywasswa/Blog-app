<h1>New Post</h1>

<%= form_with(model: @post, url: user_posts_path) do |form| %>
    <div class="form-post">
    <div class="label1">
    <%= form.label :title %>
    <%= form.text_field :title %>
    </div>
    <div class="label2">
    <%= form.label :text %> 
    <%= form.text_field :text %>
    </div>
<div class="post-btn">
<p><%=form.submit "Create Post", class:"new-btn"%></p>
</div>
</div>
<% end %>

<%= flash[:error] %>
